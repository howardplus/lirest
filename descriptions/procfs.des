[
{
  "name": "process cmdline",
  "system": {
    "source": {
      "type": "procfs",
      "path": "/proc/{pid}/cmdline",
      "refresh": "never"
    },
    "rdFormat": {
      "type": "asis",
      "multiline": false
    }
  },
  "api": {
    "path": "/proc/{pid}/cmdline",
    "methods": ["GET"],
    "descriptions":
    [
      {
        "method": "GET",
        "short": "Get kernel command line",
        "long": "Get kernel bootup command argument"
      }
    ]
  },
  "vars":
  [ 
    {
      "name": "pid",
      "dataType": "uint"
    }
  ] 
},
